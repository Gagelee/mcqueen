/**
 * This file will contain code to talk over I2C
 *
 **/



void i2c_init (int led) {
  // Blink led 
  digitalWrite (led, HIGH);
  delay (50);
  digitalWrite (led, LOW);
  delay (50);
  digitalWrite (led, HIGH);
  delay (50);
  digitalWrite (led, LOW);
  delay (50);
  digitalWrite (led, HIGH);
  delay (50);
  digitalWrite (led, LOW);
  delay (50);
  digitalWrite (led, HIGH);
  delay (50);
  digitalWrite (led, LOW);
  delay (50);
  
}
